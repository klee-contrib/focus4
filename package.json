{
  "name": "focus4",
  "version": "8.7.2-rc",
  "description": "Focus v4 (Typescript + MobX)",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/get-focus/focus4.git"
  },
  "author": "damien.frikha@kleegroup.com",
  "license": "MIT",
  "homepage": "https://github.com/get-focus/focus4",
  "scripts": {
    "clean": "node scripts/clean.js",
    "prepublishOnly": "node scripts/clean.js && node scripts/tcm.js && tsc --outDir ./ && node scripts/copy-css.js ./",
    "postpublish": "rm -rf config.js config.d.ts index.js index.d.ts ioc.js ioc.d.ts reference.js reference.d.ts user.js user.d.ts util.js util.d.ts && rm -rf application collections components entity layout message network router style testing translation",
    "start": "tsc && node scripts/copy-css.js dist",
    "stylelint": "stylelint src/**/*.css",
    "tcm": "node scripts/tcm.js",
    "test": "set TS_NODE_COMPILER_OPTIONS={\"module\":\"commonjs\"} && ts-node ./node_modules/tape/bin/tape src/entity/__test__/index.ts | tap-spec",
    "testgen": "set TS_NODE_COMPILER_OPTIONS={\"module\":\"commonjs\"} && ts-node src/testing/generator.ts src ..",
    "tslint": "tslint --project tsconfig.json --force"
  },
  "dependencies": {
    "@types/i18next": "8.4.3",
    "@types/lodash": "4.14.100",
    "@types/numeral": "0.0.22",
    "@types/prop-types": "15.5.2",
    "@types/react": "15.0.1",
    "@types/react-dnd": "2.0.34",
    "@types/react-dnd-html5-backend": "2.1.8",
    "@types/react-dom": "15.5.6",
    "@types/react-motion": "0.0.22",
    "@types/uuid": "3.4.3",
    "core-decorators": "0.20.0",
    "i18next": "10.3.0",
    "inputmask-core": "2.2.0",
    "inversify": "4.10.0",
    "inversify-inject-decorators": "3.1.0",
    "lodash": "4.17.4",
    "lodash-decorators": "4.5.0",
    "material-design-icons-iconfont": "3.0.3",
    "mobx": "3.4.1",
    "mobx-react": "4.3.5",
    "moment": "2.20.1",
    "numeral": "2.0.6",
    "react": "15.6.2",
    "react-css-themr": "2.1.2",
    "react-dnd": "2.5.4",
    "react-dnd-html5-backend": "2.5.4",
    "react-dom": "15.6.2",
    "react-motion": "0.5.2",
    "react-text-mask": "5.1.0",
    "react-toolbox": "2.0.0-beta.12",
    "reflect-metadata": "0.1.12",
    "smoothscroll-polyfill": "0.4.0",
    "text-mask-core": "5.0.1",
    "tslib": "1.9.0",
    "uuid": "3.2.1",
    "yester": "0.10.0"
  },
  "devDependencies": {
    "@types/shelljs": "0.7.8",
    "@types/tape": "4.2.31",
    "shelljs": "0.8.1",
    "stylelint": "8.4.0",
    "tap-spec": "4.1.1",
    "tape": "4.8.0",
    "ts-node": "4.1.0",
    "tslint": "5.9.1",
    "typed-css-modules": "0.3.1",
    "typescript": "2.7.1"
  }
}
